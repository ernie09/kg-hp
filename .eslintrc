{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "html",
    "react",
    "react-native"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "globals": {
    "ol": false
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"],
    "semi": ["error", "always"],
    "key-spacing": ["error", {
      "beforeColon": false,
      "afterColon": true
    }]
  }
}
